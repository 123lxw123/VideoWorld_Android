<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseColor">
    <attr name="color_BBE6DE" format="color" />
    <attr name="color_16B998" format="color" />
    <!--基本色 START-->
    <attr name="com_wm_bg" format="color" />
    <attr name="com_space_line" format="color" />
    <attr name="com_main_A" format="color" />
    <attr name="com_main_B" format="color" />
    <attr name="com_assist_A" format="color" />
    <attr name="com_assist_B" format="color" />
    <attr name="com_font_A" format="color" />
    <attr name="com_font_B" format="color" />
